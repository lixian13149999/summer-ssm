.sidebar-cont(data-cont="menu-cont")
    .menu-box
        each menu,index in userMenus
            dl.menu
                dt.menu-title
                    i.menu-icon.icon(class="#{menu.icon}")
                    a.title-cont(href="javascript:void(0)") #{menu.name}
                    each item,index2 in menu.childs
                        dd.menu-item(data-page-click="menu-item")
                            a.item-cont(data-menu-side="item" data-menu="item" data-body-mapping="#{item.href}" href="javascript:void(0)") #{item.name}
